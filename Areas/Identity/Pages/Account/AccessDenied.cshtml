@page
@model AccessDeniedModel
@{
    ViewData["Title"] = "Access denied";
}


<center>
    <header class="mt-5">
        <i class="fas fa-exclamation-triangle fa-lg text-danger" style="font-size:100px"></i><br />
        <h1 class="text-danger display-2">@ViewData["Title"]</h1>
        <h4 class="text-danger">You do not have access to this resource.</h4>
    </header>

</center>
